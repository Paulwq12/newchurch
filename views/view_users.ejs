<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>View Users</title>
  <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
  <header>
    <h1>Registered Users</h1>
  </header>
  <main>
    <table>
      <thead>
        <tr>
          <th>Username</th>
          <th>Registration Time</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach(user => { %>
          <tr>
            <td><%= user.username %></td>
            <td><%= user.registrationTime %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </main>
</body>
</html>
